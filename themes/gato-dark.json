{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "author": "DCCXXV",
    "name": "Gato Dark",
    "themes": [
        {
            "name": "Gato Dark",
            "appearance": "dark",
            "style": {
                "background": "#051315",
                "border": "#1a3a3e",
                "border.variant": "#1a3a3e",
                "border.focused": "#FF7A7A",
                "text": "#F2F2F2",
                "text.muted": "#8A9EA0",
                "text.accent": "#FF7A7A",

                "status_bar.background": "#051315",
                "title_bar.background": "#051315",
                "title_bar.inactive_background": "#051315",
                "tab_bar.background": "#051315",
                "tab.active_background": "#051315",
                "tab.inactive_background": "#051315",
                "tab.active_border": "#FF7A7A",

                "tab_bar.border": "#1a3a3e",
                "toolbar.background": "#051315",
                "toolbar.border": "#FF7A7A",
                "editor.subheader.background": "#051315",

                "elevated_surface.background": "#0a1d20",
                "surface.background": "#0a1d20",
                "menu.background": "#0a1d20",
                "menu.border": "#1a3a3e",
                "popover.background": "#0a1d20",
                "popover.border": "#1a3a3e",
                "overlay.background": "#0a1d20",
                "panel.background": "#0a1d20",
                "panel.focused_border": "#FF7A7A",

                "element.background": "#1a3a3e30",
                "element.hover": "#1a3a3e60",
                "element.active": "#1a3a3e80",
                "element.selected": "#FF7A7A40",
                "ghost_element.hover": "#1a3a3e40",
                "ghost_element.selected": "#FF7A7A30",

                "terminal.background": "#051315",

                "editor.foreground": "#F2F2F2",
                "editor.background": "#051315",
                "editor.gutter.background": "#051315",
                "editor.line_number": "#365357",
                "editor.active_line_number": "#FF7A7A",
                "editor.active_line.background": "#1a3a3e40",

                "syntax": {
                    "comment": { "color": "#5C787C", "font_style": "italic" },
                    "constant": { "color": "#FAD074" },
                    "function": { "color": "#79D6E3", "font_weight": 600 },
                    "keyword": { "color": "#FF7A7A", "font_weight": 700 },
                    "number": { "color": "#FAD074" },
                    "operator": { "color": "#8eb2b9" },
                    "punctuation": { "color": "#8eb2b9" },
                    "string": { "color": "#A8E57C" },
                    "type": { "color": "#FFCC66", "font_weight": 600 },
                    "variable": { "color": "#F2F2F2" },
                    "property": { "color": "#79D6E3" }
                }
            }
        }
    ]
}
